  <link rel="stylesheet" href="/css/home.css">

<body>
  <h1>Weekly Habits</h1>
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Habit Name</th>
      </tr>
    </thead>
    <tbody>
      <% for (let habit of habits) { %>
        <tr>
          <td><%= habit.sdate %></td>
          <td><%= habit.name %></td>
        </tr>
        <% } %> 
    </tbody>
  </table>
  <p>
    <a href="/nextWeek">Next Week</a>
    <a href="/previousWeek">Previous Week</a>
  </p>
    
</body>




</script>
